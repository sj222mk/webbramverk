<h1>
  <% @name %>
</h1>

<div class="col-md-8">
    <% if @api_keys.any? %>
      <h3>Api-keys (<%= @user.api_keys.count %>)</h3>
      <table class="api-keys">
        <tr>
          <th>Description</th>
          <th>Api-key</th>
          <th>Expires at</th>
        </tr>
        
        <%= render @api_keys %>
      </table>
    <%= will_paginate @api_keys %>
  <% end %>
</div>